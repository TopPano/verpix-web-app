  "main": "",
  "scripts": {
    "clean": "rimraf public/static/build/*",
    "build": "NODE_ENV='production' webpack -p --progress",
    "lint": "eslint ./shared",
    "posttest": "npm run lint",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "dev": "NODE_ENV='development' node server/index.js",
    "start": "NODE_ENV='production' pm2 start server/index.js --name verpix-web-app",
    "stop": "NODE_ENV='production' pm2 delete verpix-web-app -s &> /dev/null",
    "test": "NODE_ENV='test' karma start",
    "test:watch": "NODE_ENV='test' karma start --autoWatch=true --singleRun=false"
  },
